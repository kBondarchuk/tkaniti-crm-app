{
  "root": true,
  "env": {
    "node": true,
    "jquery": true,
    "es2021": true
  },
  "extends": [
    // "plugin:vue/vue3-essential",
    // "plugin:vue/vue3-strongly-recommended",
    "plugin:vue/vue3-recommended",
    // "plugin:vue/essential",
    // "eslint:recommended",
    "@vue/prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "vue/no-v-html" : "off",
    // "vue/require-default-prop" : "off",
    // "vue/prop-name-casing" : "off",
    // "vue/require-prop-types" : "off",
    "prettier/prettier": [
      "warn",
      {
        "printWidth": 120
      }
    ]
  }
}
